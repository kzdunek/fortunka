//= require jquery
//= require jquery_ujs
//= require_self


  $('#fortunes_search, nav a').live('ajax:error',function(event,xhr, status, error){
	$('#content').html(xhr.responseText);
  
   });
 $('#fortunes_search, nav a').live('ajax:success',function(event,data, status, error){
	$('#content').html(data);
  
   });
